<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="251.0" prefWidth="268.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.dictionary.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button onAction="#onHelloButtonClick" text="Hello!" />

    <Label fx:id="welcomeText" contentDisplay="BOTTOM" prefHeight="30.0" prefWidth="80.0" text="Click me">
      <font>
         <Font name="System Bold" size="20.0" />
      </font></Label>
   <Button mnemonicParsing="false" text="Click!" />
</VBox>
